{
  "name": "aiproducer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run lint && next build",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "start": "next start",
    "lint": "next lint",
    "studio": "npx dotenv -e .env.development -- prisma studio",
    "generate": "npx dotenv -e .env.development -- prisma generate",
    "db-push": "npx dotenv -e .env.development -- prisma db push --force-reset",
    "db-seed": "npm run db-push && npx dotenv -e .env.development -- prisma db seed",
    "migrate:postgres": "npx dotenv -e .env.local -- prisma migrate deploy",
    "migrate:dev": "npx dotenv -e .env.development -- prisma migrate dev",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "@fullstory/browser": "^2.0.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.1",
    "@prisma/client": "^5.2.0",
    "@vercel/analytics": "^1.1.1",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "eslint": "8.46.0",
    "eslint-config-next": "^14.0.3",
    "framer-motion": "^10.16.2",
    "next": "^14.0.3",
    "gray-matter": "^4.0.3",
    "openai": "^4.12.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-select": "^5.7.4",
    "react-slick": "^0.29.0",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "slick-carousel": "^1.8.1",
    "stripe": "^12.18.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.3.1",
    "@tailwindcss/forms": "^0.5.6",
    "autoprefixer": "^10.4.15",
    "dotenv-cli": "^7.3.0",
    "eslint-config-prettier": "^9.0.0",
    "postcss": "^8.4.28",
    "prisma": "^5.2.0",
    "tailwindcss": "^3.3.3"
  }
}
