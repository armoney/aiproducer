{
  "name": "aiproducer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run lint && npm run generate && npm run db-push && npm run db-seed && next build",
    "start": "next start",
    "lint": "next lint",
    "studio": "npx dotenv -e .env.local -- prisma studio",
    "generate": "npx dotenv -e .env.local -- prisma generate",
    "db-push": "npx dotenv -e .env.local -- prisma db push --force-reset",
    "db-seed": "npm run db-push && npx dotenv -e .env.local -- prisma db seed",
    "migrate:postgres": "npx dotenv -e .env.local -- prisma migrate deploy"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.1",
    "@prisma/client": "^5.2.0",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.13",
    "framer-motion": "^10.16.2",
    "next": "13.4.13",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.4",
    "react-select": "^5.7.4",
    "stripe": "^12.18.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.3.1",
    "@tailwindcss/forms": "^0.5.6",
    "autoprefixer": "^10.4.15",
    "dotenv-cli": "^7.3.0",
    "postcss": "^8.4.28",
    "prisma": "^5.2.0",
    "tailwindcss": "^3.3.3"
  }
}
